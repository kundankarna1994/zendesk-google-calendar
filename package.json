{
  "name": "react_js-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "watch": "vite build --watch --mode development",
    "build:dev": "vite build --mode development",
    "build": "vite build --mode production",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "prepare": "husky",
    "start": "zcli apps:server src"
  },
  "dependencies": {
    "@zendeskgarden/css-bedrock": "^9.*",
    "@zendeskgarden/react-buttons": "^9.*",
    "@zendeskgarden/react-grid": "^9.*",
    "@zendeskgarden/react-theming": "^9.*",
    "@zendeskgarden/react-typography": "^9.*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.2.1",
    "fast-glob": "^3.3.3",
    "husky": "^9.1.7",
    "typescript": "^5.8.2",
    "vite": "^5.2.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "biome format --write",
      "biome check --write"
    ]
  }
}
